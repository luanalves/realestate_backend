<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- CHK002: Cron job to auto-expire active leases past end_date -->
        <record id="ir_cron_expire_leases" model="ir.cron">
            <field name="name">Lease: Auto-expire past end date</field>
            <field name="model_id" ref="model_real_estate_lease"/>
            <field name="state">code</field>
            <field name="code">model._cron_expire_leases()</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="active" eval="True"/>
            <field name="doall" eval="False"/>
        </record>
    </data>
</odoo>
