<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Replace Favicon -->
    <template id="layout_favicon" inherit_id="web.layout" name="Custom Favicon">
        <xpath expr="//link[@rel='shortcut icon']" position="replace">
            <link rel="shortcut icon" href="/thedevkitchen_branding/static/src/img/favicon.ico" type="image/x-icon"/>
        </xpath>
    </template>
</odoo>
